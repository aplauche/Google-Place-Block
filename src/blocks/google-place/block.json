{
  "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
  "apiVersion": 2,
  "name": "fsdgpb/google-place",
  "title": "Place Information",
  "category": "text",
  "description": "Adds an information box for a google place listing via API",
  "textdomain": "fsdgpb",
  "attributes": {
    "lat": {
      "type": "string",
      "default": ""
    },
    "lng": {
      "type": "string",
      "default": ""
    },
    "rating": {
      "type": "integer",
      "default": 0
    },
    "price": {
      "type": "integer",
      "default": 0
    },
    "address": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "align": ["wide"]
  },
  "editorScript": "file:./index.js",
  "script": "file:./frontend.js",
  "style": "file:./index.css"
}